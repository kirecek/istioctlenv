#!/usr/bin/env bash

set -uo pipefail;

echo 'Usage: istioctlenv <command> [<options>]

Commands:
   cd            Switch cwd to a specific version of istioctl installation files
   install       Install a specific version of istioctl
   use           Switch a version to use
   uninstall     Uninstall a specific version of istioctl
   list          List all installed versions
   list-remote   List all installable versions
';

exit 0;
